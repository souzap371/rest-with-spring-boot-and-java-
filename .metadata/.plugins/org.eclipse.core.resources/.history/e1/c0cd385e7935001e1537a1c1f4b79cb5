package br.ufrn.imd.solicitacaurn.solus.repository;

import java.util.Optional;
import java.util.UUID;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import br.com.mkapi.solus.domain.ArquivoSolus;

@Repository
public interface ArquivoSolusRepository extends JpaRepository<ArquivoSolus, Long>{

	@Query(value = "select * from ARQUIVOS where ativo=true and id=?1", nativeQuery = true )
	public Optional<ArquivoSolus> findById(UUID id);
}
