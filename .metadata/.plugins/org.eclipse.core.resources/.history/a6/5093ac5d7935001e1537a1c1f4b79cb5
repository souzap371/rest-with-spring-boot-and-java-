package br.ufrn.imd.solicitacaurn.cassi.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import br.com.mkapi.cassi.domain.TipoPrestadorSoc;

@Repository
public interface TipoPrestadorSocRepository extends JpaRepository<TipoPrestadorSoc, Long>{
    @Query(value = "SELECT * FROM TIPO_PRESTADOR_SOC where ativo = 1 ORDER BY descricao", nativeQuery = true)
    public List<TipoPrestadorSoc> getAllAtivo();
}
