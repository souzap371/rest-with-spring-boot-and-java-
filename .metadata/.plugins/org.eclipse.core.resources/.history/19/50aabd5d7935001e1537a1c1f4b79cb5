package br.ufrn.imd.solicitacaurn.firebase.domain;

import br.com.mkapi.solus.domain.enums.TipoSolicitacao;

public class PushNotificationRequest {

	private String title;
    private String message;
    private String topic;
    private String token;
    private TipoSolicitacao tipoSolicitacao;
    private String idSolicitacao;
    private Long idNoticia;
    private Long idUsuario;

    public PushNotificationRequest() {
    }

    public PushNotificationRequest(String title, String messageBody, String topicName, TipoSolicitacao tipoSolicitacao, String idSolicitacao) {
        this.title = title;
        this.message = messageBody;
        this.topic = topicName;
        this.tipoSolicitacao = tipoSolicitacao;
        this.idSolicitacao = idSolicitacao;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }

    public String getMessage() {
        return message;
    }

    public void setMessage(String message) {
        this.message = message;
    }

    public String getTopic() {
        return topic;
    }

    public void setTopic(String topic) {
        this.topic = topic;
    }

    public String getToken() {
        return token;
    }

    public void setToken(String token) {
        this.token = token;
    }

	public TipoSolicitacao getTipoSolicitacao() {
		return tipoSolicitacao;
	}

	public void setTipoSolicitacao(TipoSolicitacao tipoSolicitacao) {
		this.tipoSolicitacao = tipoSolicitacao;
	}

	public String getIdSolicitacao() {
		return idSolicitacao;
	}

	public void setIdSolicitacao(String idSolicitacao) {
		this.idSolicitacao = idSolicitacao;
	}

    public Long getIdNoticia() {
		return idNoticia;
	}

	public void setIdNoticia(Long idNoticia) {
		this.idNoticia = idNoticia;
	}

    public Long getIdUsuario() {
		return idUsuario;
	}

	public void setIdUsuario(Long idUsuario) {
		this.idUsuario = idUsuario;
	}    
}
