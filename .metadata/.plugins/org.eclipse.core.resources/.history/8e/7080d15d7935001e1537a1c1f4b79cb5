package br.ufrn.imd.solicitacaurn.mkdata.repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import br.com.mkapi.mkdata.domain.EspecialidadeMkdata;


@Repository
public interface EspecialidadeRepositoryMkdata extends JpaRepository<EspecialidadeMkdata, Long> {
	
	
	@Query(value = "select * from especialidades where ativo = 'S' and CBOS is not NULL "
			+ "order by nome ASC", nativeQuery = true)
	public List<EspecialidadeMkdata> getAll();

}
